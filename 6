#include <stdio.h>
#include <math.h>
int main() {
    int x1, y1, x2, y2;
    float length;
    //Введення даних
    printf("Введіть перші точки вектора(x y):");
    scanf("%d %d",&x1,&y1);
    printf("Введіть другі точки вектора (x y):");
    scanf("%d %d", &x2, &y2);
    //Використовуємо алгоритм для пошуку довжини вектора
    //Від кординат кінця віднімаємо координати початку
    int dx=x2-x1;
    int dy=y2-y1;
    //Обчислюємо довжину вектора за формулою
     length = sqrt(dx * dx + dy * dy);
    // Виводимо результат з точністю до шести знаків після коми
    printf("Довжина вектора: %.6f\n",length);
    return 0;
}
