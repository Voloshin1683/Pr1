#include <stdio.h>
int main() {
int d, i,count=0;
//Введення числа
printf("Введіть число: ");
scanf("%d",&d);
//Додаємо умову
if(d>1&&d<150){
    for(i=1;i<d;i++){
    int qu=d/i;// Знаходемо частку
    int rem=d%i;// Знаходемо залишок
    //Скільки раз виконується наша умова
    if (qu == rem) {
    count++;}
    }
//Виводимо результат
printf("Кількість рівних дільників числа %d: %d\n", d, count);
}else printf("Помилка");
}
